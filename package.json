    "@types/node": "^16.11.7",
    "@types/uuid": "^8.3.3",
    "jest": "^27.3.1",
    "jest-coverage-badges": "^1.1.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
